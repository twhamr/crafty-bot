# twhamr - Discord Bot (CraftyBot) w/ custom API Integration for Crafty Controller
# main.ini template file, necessary for initial run and setting up application

# General
[general]
app_name = "CraftyBot"
version = 0.0.0

# Crafty API
[api]
username = YOUR_CRAFTY_USERNAME
password = YOUR_CRAFTY_PASSWORD
api_url = YOUR_CRAFTY_API_URL
api_key = YOUR_CRAFTY_API_KEY

# Discord
[discord]
enabled = true
bot_token = DISCORD_BOT_TOKEN

# Logging
[logging]
enabled = true
dir_path = ./logs